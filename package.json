  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230914.0",
    "@types/express": "^4.17.14",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "nodemon": "^2.0.15",
